{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest", "tslint-react"
  ],
  "jsRules": {},
  "rules": {
    "trailing-comma": [false],
    "indent": [true, "spaces", 4],
    "no-console": [false],
    "quotemark": [true, "single"],
    "ordered-imports": [false],
    "no-namespace": [false],
    "interface-name": [true, "never-prefix"],
    "jsx-no-multiline-js": [false]
  },
  "rulesDirectory": []
}
